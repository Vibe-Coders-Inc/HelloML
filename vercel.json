{
    "version": 2,
    "functions": {
      "webhook.py": {
        "runtime": "python3.9"
      }
    },
    "builds": [
      {
        "src": "webhook.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "webhook.py"
      }
    ]
  }